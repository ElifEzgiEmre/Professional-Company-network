@model List<Proje1.Models.Education> 
@{
    ViewData["Title"] = "Educationlist";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Educationlist</h1>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Category</th>
    </tr>

    @foreach(var edu in Model)
    {
        <tr>
            <td>@edu.DegreeLevel</td>
            <td>@edu.UniversityName</td>
            <td>@edu.StartDate - @edu.TerminationDate</td>
        </tr>
    }
</table>